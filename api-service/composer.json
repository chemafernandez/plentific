{
    "name": "chema/api-service",
    "version": "1.0.0",
    "description": "A PHP service for retrieving and creating users via API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Chema\\ApiService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chema\\ApiService\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Chema Fernandez"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.4",
        "guzzlehttp/guzzle": "8.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^13.0@dev",
        "squizlabs/php_codesniffer": "4.x-dev",
        "phpstan/phpstan": "2.1.x-dev"
    }
}
